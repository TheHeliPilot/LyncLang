// Note: This features Phase 3 syntax which is currently in the roadmap
def main(): int {
    maybe_ptr: own? int = alloc 42;

    i: int = 5;

    match maybe_ptr {
        some(f): {
            i = f;
        }
        null: {}
    };
    i = match maybe_ptr {
        some(f): f;
        null: 0;
    };

    free maybe_ptr;
    return 0;
}