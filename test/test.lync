include std.io.*;

def main(): int {
    print("Enter a float: ");
    f_opt: own? float = read_float();
    
    match f_opt {
        some(val): {
            print("You entered float: ", val);
        }
        null: {
            print("Failed to read float");
        }
    };

    free f_opt;

    print("Enter a double: ");
    d_opt: own? double = read_double();

    match d_opt {
        some(val): {
            print("You entered double: ", val);
        }
        null: {
            print("Failed to read double");
        }
    };

    free d_opt;

    return 0;
}
