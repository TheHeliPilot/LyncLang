
extern <math.h> {
  def sqrt(x: double): double;
}

def main(): int {

    print("Testing Floats and Doubles...");

    f: float = 3.14f;
    print("f (float) = ", f);

    d: double = 123.456;
    print("d (double) = ", d);

    res_float: float = 1 + f;
    print("1 + f = ", res_float);

    res_double: double = f + d;
    print("f + d = ", res_double);

    print("sqrt(16.0) = ", sqrt(16.0));

    return 0;
}